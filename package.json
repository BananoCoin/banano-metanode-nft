{
  "name": "banano-metanode-nft",
  "version": "0.2.1",
  "description": "banano metanode nft",
  "author": "Coranos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BananoCoin/banano-metanode-nft.git"
  },
  "scripts": {
    "coverage": "nyc -x 'test/util/**' -x 'test/unit/**' ./node_modules/mocha/bin/mocha -t 100000 test/unit/**",
    "coverage-verbose ": "nyc  --reporter=lcov  -x 'test/util/**' -x 'test/unit/**' ./node_modules/mocha/bin/mocha -t 100000 test/unit/**",
    "docs": "jsdoc2md documentation.js scripts/**/*.js > docs/documentation.md",
    "eslint": "eslint --fix '**/*.js'",
    "npm-check-updates": "npm-check-updates --target minor -u",
    "preflight": "npm update && npm run npm-check-updates && npm i && npm audit fix && npm run eslint && npm run prettier && npm run coverage && npm run docs && npm outdated",
    "prettier": "prettier \"**/**.{ts,js,json,css,scss,html}\" --single-quote --write",
    "screenjoin": "screen -x banano_metanode_nft;exit 0;",
    "screenstart": "screen -dmSL banano_metanode_nft npm start;npm run screenjoin;",
    "screenrestart": "npm stop;npm run screenjoin;npm run screenstart;",
    "start": "node --trace-warnings scripts/index.js",
    "stop": "node scripts/stop.js",
    "touch": "find . -exec touch {} \\",
    "test": "./node_modules/mocha/bin/mocha --recursive -t 100000 test/unit",
    "itest": "./node_modules/mocha/bin/mocha --recursive -t 100000 test/integration"
  },
  "dependencies": {
    "@bananocoin/banano-ipfs": "^1.1.2",
    "@bananocoin/bananojs": "^2.5.3",
    "abort-controller": "^3.0.0",
    "await-semaphore": "^0.1.3",
    "bs58": "^5.0.0",
    "express": "^4.17.3",
    "node-fetch": "^2.6.7",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1"
  },
  "preferGlobal": true,
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc-to-markdown": "^7.1.1",
    "mocha": "^9.2.1",
    "npm-check-updates": "^12.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1"
  }
}
